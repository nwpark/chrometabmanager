{
  "manifest_version": 2,
  "name": "Chrome Tab Manager",
  "version": "0.1.0",

  "description": "Manage lots of tabs!",
  "icons": { "128": "assets/icon128.png" },

  "browser_action": {
    "default_title": "Chrome tab manager version 0.1"
  },

  "permissions": ["tabs", "storage"],

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
