{
  "manifest_version": 2,

  "name": "Tab Manager",
  "description": "Manage lots of tabs!",
  "version": "0.0.1",

  "permissions": ["tabs", "storage"],

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
